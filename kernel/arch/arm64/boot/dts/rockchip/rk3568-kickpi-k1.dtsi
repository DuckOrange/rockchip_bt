/ {
	model = "Rockchip RK3568 KICKPI K1A Board";
	compatible = "rockchip,rk3568-kickpi-k1a", "rockchip,rk3568";

	vcc2v5_sys: vcc2v5-ddr {
		compatible = "regulator-fixed";
		regulator-name = "vcc2v5-sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <2500000>;
		regulator-max-microvolt = <2500000>;
		vin-supply = <&vcc3v3_sys>;
	};

	vcc3v3_bu: vcc3v3-bu {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_bu";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&vcc5v0_sys>;
	};
/*
	fan0: gpio-fan {
		#cooling-cells = <2>;
		compatible = "gpio-fan";
		gpio-fan,speed-map = <0 0 
								3000 1>;
		gpios = <&gpio0 RK_PD6 GPIO_ACTIVE_HIGH>;
		status = "okay";
	};
	*/
};

&leds {
    fan {
        gpios = <&gpio0 RK_PD6 GPIO_ACTIVE_HIGH>;
        default-state = "on";
    };
};

&rk809_sound {
	spk-det-gpio = <&gpio4 RK_PC6 GPIO_ACTIVE_LOW>;
	hp-det-gpio = <&gpio1 RK_PD3 GPIO_ACTIVE_HIGH>;
	pinctrl-names = "default";
	pinctrl-0 = <&hp_det>;
	hp-volume = <20>;
	spk-volume = <30>;
};

&i2c5 {
	status = "okay";

	hym8563: hym8563@51 {
		compatible = "haoyu,hym8563";
		reg = <0x51>;
		#clock-cells = <0>;
		clock-frequency = <32768>;
		clock-output-names = "hym8563";
		// IRQ GPIO IS MODIFYED FOR UPDATE HYM8562 DRIVER 
		rtc-irq-gpio = <&gpio0 RK_PD3 IRQ_TYPE_EDGE_FALLING>;
	};
	
};

&pinctrl {
	headphone {
		hp_det: hp-det {
			rockchip,pins = <1 RK_PD3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};
};
